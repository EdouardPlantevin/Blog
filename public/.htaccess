RewriteEngine On

RewriteRule ^admin/liste-des-articles$ /Blog/public/admin/articles [L,QSA]

RewriteRule ^admin/liste-des-commentaires$ /Blog/public/admin/comments [L,QSA]

RewriteRule ^admin/liste-des-contacts$ /Blog/public/admin/contacts [L,QSA]

RewriteRule ^admin/liste-des-articles/edition/([0-9]*)$ /Blog/public/admin/editArticle/$1 [L,QSA]

RewriteRule ^liste-des-articles$ /Blog/public/articles [L,QSA]

RewriteRule ^articles/detail/([0-9]*)$ /Blog/public/articles/show/$1 [L,QSA]

RewriteRule ^edouard-plantevin$ /Blog/public/ [L,QSA]

RewriteRule ^([a-zA-Z0-9\-\_\/]*)$ index.php?p=$1
